{
  "name": "pixijs-mouse-event-transmitter",
  "version": "0.2.3",
  "description": "Transmit mouse events from pixi.js stage to other canvas.",
  "main": "./lib/index.js",
  "types": "./lib/index.d.ts",
  "module": "./esm/index.js",
  "files": [
    "lib",
    "esm"
  ],
  "repository": "https://MasatoMakino@github.com/MasatoMakino/pixijs-mouse-event-transmitter.git",
  "author": "MasatoMakino <unagiinu.g@gmail.com>",
  "license": "MIT",
  "devDependencies": {
    "@types/jest": "^26.0.24",
    "@types/node": "^15.14.9",
    "canvas": "^2.9.1",
    "fake-mouse-event": "github:MasatoMakino/fake-mouse-event",
    "gulptask-demo-page": "github:MasatoMakino/gulptask-demo-page",
    "gulptask-dev-server": "github:MasatoMakino/gulptask-dev-server",
    "husky": "^6.0.0",
    "jest": "^27.5.1",
    "pixi.js-legacy": "^6.3.0",
    "prettier": "^2.6.2",
    "pretty-quick": "^3.1.3",
    "ts-jest": "^27.1.4",
    "ts-node": "^10.7.0",
    "typedoc": "^0.22.15",
    "typescript": "^4.6.3"
  },
  "peerDependencies": {
    "pixi.js": "^6.3.0"
  },
  "scripts": {
    "prepare": "husky install",
    "test": "jest",
    "coverage": "jest --coverage",
    "start:dev": "npm run server & npm run watch:tsc & npm run watch:demo",
    "buildTS": "npx tsc --project tsconfig.cjs.json & npx tsc --project tsconfig.esm.json",
    "watch:tsc": "tsc -W --incremental --project tsconfig.cjs.json & tsc -W --incremental --project tsconfig.esm.json",
    "demo": "npx gulptask-demo-page",
    "watch:demo": "npm run demo -- -W",
    "server": "npx gulptask-dev-server -b ./docs/demo",
    "typedocs": "npx typedoc --out ./docs/api src/index.ts"
  },
  "dependencies": {}
}
