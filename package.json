{
  "name": "@masatomakino/pixijs-mouse-event-transmitter",
  "version": "0.2.3",
  "description": "Transmit mouse events from pixi.js stage to other canvas.",
  "main": "./lib/index.js",
  "types": "./lib/index.d.ts",
  "module": "./esm/index.js",
  "files": [
    "lib",
    "esm"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://MasatoMakino@github.com/MasatoMakino/pixijs-mouse-event-transmitter.git"
  },
  "author": "MasatoMakino <unagiinu.g@gmail.com>",
  "license": "MIT",
  "devDependencies": {
    "@types/jest": "^28.1.1",
    "@types/node": "^17.0.40",
    "canvas": "^2.9.1",
    "@masatomakino/fake-mouse-event": "0.0.8",
    "@masatomakino/gulptask-demo-page": "0.4.4",
    "@masatomakino/gulptask-dev-server": "0.4.1",
    "husky": "^8.0.1",
    "jest": "^28.1.1",
    "jest-environment-jsdom": "^28.1.0",
    "pixi.js-legacy": "^6.4.2",
    "prettier": "^2.6.2",
    "pretty-quick": "^3.1.3",
    "ts-jest": "^28.0.4",
    "ts-node": "^10.8.1",
    "typedoc": "^0.22.17",
    "typescript": "^4.7.3"
  },
  "peerDependencies": {
    "pixi.js": "^6.4.2"
  },
  "scripts": {
    "prepare": "husky install",
    "test": "jest",
    "coverage": "jest --coverage",
    "start:dev": "npm run server & npm run watch:tsc & npm run watch:demo",
    "buildTS": "npx tsc --project tsconfig.cjs.json & npx tsc --project tsconfig.esm.json",
    "watch:tsc": "tsc -W --incremental --project tsconfig.cjs.json & tsc -W --incremental --project tsconfig.esm.json",
    "demo": "npx @masatomakino/gulptask-demo-page",
    "watch:demo": "npm run demo -- -W",
    "server": "npx @masatomakino/gulptask-dev-server -b ./docs/demo",
    "typedocs": "npx typedoc --out ./docs/api src/index.ts"
  },
  "bugs": {
    "url": "https://github.com/MasatoMakino/pixijs-mouse-event-transmitter/issues"
  },
  "homepage": "https://github.com/MasatoMakino/pixijs-mouse-event-transmitter#readme",
  "directories": {
    "doc": "docs",
    "lib": "lib"
  }
}
