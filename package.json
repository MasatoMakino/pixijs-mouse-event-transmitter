{
  "name": "@masatomakino/pixijs-mouse-event-transmitter",
  "version": "0.4.1",
  "description": "Transmit mouse events from pixi.js stage to other canvas.",
  "main": "./lib/index.js",
  "types": "./lib/index.d.ts",
  "module": "./esm/index.js",
  "files": [
    "lib",
    "esm"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://MasatoMakino@github.com/MasatoMakino/pixijs-mouse-event-transmitter.git"
  },
  "author": "MasatoMakino <unagiinu.g@gmail.com>",
  "license": "MIT",
  "devDependencies": {
    "@masatomakino/fake-mouse-event": "^0.0.9",
    "@masatomakino/gulptask-demo-page": "^0.6.1",
    "@types/jest": "^29.5.0",
    "@types/node": "^18.15.3",
    "browser-sync": "^2.29.0",
    "canvas": "^2.11.0",
    "husky": "^8.0.3",
    "jest": "^29.5.0",
    "jest-environment-jsdom": "^29.5.0",
    "pixi.js-legacy": "^7.2.0",
    "prettier": "^2.8.4",
    "pretty-quick": "^3.1.3",
    "ts-jest": "^29.1.0",
    "ts-node": "^10.9.1",
    "typedoc": "^0.24.2",
    "typescript": "^5.0.4"
  },
  "peerDependencies": {
    "pixi.js": "^7.2.0"
  },
  "overrides": {
    "@types/css-font-loading-module": "^0.0.8"
  },
  "scripts": {
    "prepare": "husky install",
    "test": "jest",
    "coverage": "jest --coverage",
    "preversion": "git checkout main && git pull && npm run test",
    "start:dev": "npm run server & npm run watch:tsc & npm run watch:demo",
    "buildTS": "tsc --project tsconfig.cjs.json & tsc --project tsconfig.esm.json",
    "watch:tsc": "tsc -W --incremental --project tsconfig.cjs.json & tsc -W --incremental --project tsconfig.esm.json",
    "demo": "npx @masatomakino/gulptask-demo-page",
    "watch:demo": "npm run demo -- -W",
    "server": "browser-sync ./docs/demo -w",
    "typedocs": "npx typedoc --out ./docs/api src/index.ts"
  },
  "bugs": {
    "url": "https://github.com/MasatoMakino/pixijs-mouse-event-transmitter/issues"
  },
  "homepage": "https://github.com/MasatoMakino/pixijs-mouse-event-transmitter#readme",
  "directories": {
    "doc": "docs",
    "lib": "lib"
  }
}
